<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.taikang.mapper.UserMapper">
  <select id="selectUser" resultType="com.taikang.entity.Users">
    select * from users where department =#{dp}
  </select>
  <select id="selectAll" resultType="com.taikang.entity.Users">
    select * from users 
  </select>
  <select id="checkLogin" resultType="com.taikang.entity.Admin">
    select * from admin where department =#{department} and psd=#{psd}
  </select>
  <insert id="addUser">
  insert into users values(#{id},#{department},#{systemname},#{username})
  </insert>
  <delete id="deleteUser">
  delete from users where id = #{id}
  </delete>
</mapper>